--GAMEPASS --STR

local ReplicatedStorage = game:GetService("ReplicatedStorage")


local function task()
    local Event = game:GetService("ReplicatedStorage").Remotes.Dumbbell
    Event:FireServer()
end


while true do

    for i = 1, 3 do
        task()
        wait()  
    end
    wait(1.2)  
end
